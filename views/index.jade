extends layout

block content
  h1= title
  p Power meter readings
  div(id="windChart", ng-controller="readingsController")
    //highchart(id="chart", config="chartConfig")
    div(ng-repeat="(key,val) in data")
      h2 {{key}}
      ul(ng-repeat="reading in val.readings")
        li {{reading.time}} - {{reading.data}}

